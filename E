            applyHitboxesToAllPlayers()
        end
    end
}, "hbeColorPicker")

-- Cleanup function
local function cleanup()
    resetHitboxesOnAllPlayers()
    if hitboxCoroutine then
        coroutine.close(hitboxCoroutine)
    end
end

-- Store cleanup function
getgenv().HitbloxCleanup = cleanup

